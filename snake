from telethon import events 
 
import asyncio 
 
from uniborg.util import admin_cmd 

class SnakeMod(loader.Module): 
"""Змейка""" 
strings = {'name': 'Змейка'} 
 
@borg.on(admin_cmd(pattern=r"s")) 
 
async def _(event): 
 
    if event.fwd_from: 
 
        return 
 
    animation_interval = 0.3 
 
    animation_ttl = range(0, 27) 
 
    await event.edit("s") 
 
